log lammps_run.log

units real
dimension 3
atom_style full
boundary p p f
neigh_modify every 1 delay 0 check yes
kspace_style pppm 1.0e-5
kspace_modify slab 3.0

# data file written from end of classical force field simulation
read_data end_equil.data

pair_style hybrid/overlay nnp dir "/home/kdf22/projects/nnps/gen7_longrange/nnp-data" showew no showewsum 10000 maxew 10000 resetew yes calcd 1 wricde 0.0 maxcde 10000000.0 wricdf 10000000 maxcdf 10000000 cflength 1.8897261328  cfenergy 0.0015936010974213599 emap "1:C,2:O,3:H,4:Na,5:Cl" coul/long 10.0
pair_coeff * * nnp 6.35013
pair_coeff * * coul/long

set type 1 charge 0.0
set type 2 charge  -0.834
set type 3 charge 0.417
set type 4 charge 1.0
set type 5 charge -1.0

variable t equal step

group carbons type 1
group elyte type 2 3 4 5

fix fix_c carbons setforce 0.0 0.0 0.0 

compute cTemp carbons temp
compute eTemp elyte temp

dump unwrapped2 all dcd 10000 traj_unwrapped_$t.dcd 
dump_modify unwrapped2 unwrap yes 
thermo_style custom time step temp c_cTemp c_eTemp press vol density pe ke etotal spcpu cpuremain
thermo_modify flush yes
thermo 1000
#restart 1000 run1.restart run2.restart
restart 10000 run.restart
fix NVT elyte nvt temp 300 300 100.0

timestep 0.5
run 2000000 upto
write_restart nvt.restart.*
write_data end_nvt.data
write_dump all xyz end_nvt.xyz modify sort id